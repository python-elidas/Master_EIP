   "source": [
    "import hashlib as hash\n",
    "def gen_code(string):\n",
    "    model = hash.new('sha256')\n",
    "    model.update(string.encode('utf-8'))\n",
    "    hashed = model.hexdigest()\n",
    "    return str(int(hashed, 16))[:5]\n",
    "\n",
    "gen_code('hola mundo')"
   ]
  }
